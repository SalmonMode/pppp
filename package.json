{
  "name": "pppp",
  "version": "0.18.0",
  "description": "Gerald Weinberg's PPPP brought to life.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "next:build": "next build",
    "build": "TS_NODE_PROJECT='./tsconfig.test.json' npx tsc -b",
    "build:diagnostic": "TS_NODE_PROJECT='./tsconfig.test.json' npx tsc -b --extendedDiagnostics",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha --recursive ./src/",
    "test:unit": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha --recursive ./src/ -g 'Integration' -i",
    "test:integration": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha --recursive ./src/ -g 'Integration'",
    "test:integration:react": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha --recursive ./src/ -g 'React Integration'",
    "test:ci": "TS_NODE_PROJECT='./tsconfig.test.json' nyc mocha --recursive ./src/ --reporter mocha-junit-reporter"
  },
  "nyc": {
    "extends": "nyc-config-tsx",
    "extension": [
      ".ts",
      ".js",
      ".jsx",
      ".tsx"
    ],
    "include": [
      "**/*.{js,jsx,ts,tsx}"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.spec.tsx",
      "coverage",
      ".next",
      "next.config.js"
    ],
    "reporter": [
      "text",
      "html",
      "cobertura"
    ],
    "parser-plugins": [
      "jsx",
      "asyncGenerators",
      "bigInt",
      "classProperties",
      "classPrivateProperties",
      "dynamicImport",
      "importMeta",
      "objectRestSpread",
      "optionalCatchBinding"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SalmonMode/propaedeutic-reflection"
  },
  "author": "Chris NeJame",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chroma-js": "^2.1.4",
    "@types/cookie": "^0.5.1",
    "@types/enzyme": "^3.10.12",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.33.0",
    "global-jsdom": "^8.6.0",
    "jsdom": "^20.0.3",
    "mocha": "^10.1.0",
    "mocha-junit-reporter": "^2.2.0",
    "nock": "^13.2.9",
    "node-mocks-http": "^1.12.1",
    "nyc": "^15.1.0",
    "nyc-config-tsx": "^0.1.0",
    "sinon": "^14.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.7",
    "@reduxjs/toolkit": "^1.9.2",
    "@stdlib/stats-incr-mpcorr": "^0.0.6",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.29.3",
    "next": "^13.1.1",
    "primitive-predicates": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "uuid": "^9.0.0"
  }
}
